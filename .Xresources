URxvt*scrollBar: false
URxvt*depth: 32
URxvt*termName: rxvt-unicode-256color
URxvt.foreground: gray
URxvt.cursorColor: gray
URxvt.background: [85]#02020f
URxvt.font:xft:Terminus:pixelsize=30

! scrolling
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

! special
*.foreground:   #06f72e
*.background:   #020d0f
*.cursorColor:  #06f72e

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3
